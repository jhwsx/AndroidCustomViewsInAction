<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="BitmapShadowView">
        <attr name="bsv_src" format="reference" />
        <attr name="bsv_shadowDx" format="integer" />
        <attr name="bsv_shadowDy" format="integer" />
        <attr name="bsv_shadowColor" format="color" />
        <attr name="bsv_shadowRadius" format="integer" />
    </declare-styleable>
    <declare-styleable name="AvatarView">
        <attr name="av_src" format="reference" />
        <attr name="av_type" format="enum">
            <enum name="circle" value="0" />
            <enum name="rectangle" value="1" />
        </attr>
        <attr name="av_rectangle_radius" format="integer" />
    </declare-styleable>
    <declare-styleable name="PaintSetshaderLinearGradientView">
        <attr name="pslv_type" format="enum">
            <enum name="double_color" value="0" />
            <enum name="multi_color" value="1" />
        </attr>
        <attr name="pslv_x0" format="enum">
            <enum name="zero" value="0" />
            <enum name="half_width" value="1" />
            <enum name="width" value="2" />
        </attr>
        <attr name="pslv_y0" format="enum">
            <enum name="zero" value="0" />
            <enum name="half_height" value="3" />
            <enum name="height" value="4" />
        </attr>
        <attr name="pslv_x1" format="enum">
            <enum name="zero" value="0" />
            <enum name="half_width" value="1" />
            <enum name="width" value="2" />
        </attr>
        <attr name="pslv_y1" format="enum">
            <enum name="zero" value="0" />
            <enum name="half_height" value="3" />
            <enum name="height" value="4" />
        </attr>

    </declare-styleable>
    <declare-styleable name="StrokenAnimationView">
        <attr format="dimension" name="stroke_width"/>
        <attr format="dimension" name="stroke_radius"/>
        <attr format="integer" name="rotate_start"/>
        <attr format="boolean" name="is_auto_start"/>
    </declare-styleable>
</resources>